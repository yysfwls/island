.container {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.header {
    width: 750rpx;
    height: 100rpx;
    background-color: #fff;
    border-top: 1px solid #f5f5f5;
    border-bottom: 1px solid #f5f5f5;
    display: flex;
    flex-direction: row;
    align-items: center;
    position: fixed;
    z-index: 99;
}

.search-container {
    display: inline-flex;
    flex-direction: row;
    align-items: center;
    background-color: #f5f5f5;
    border-radius: 50px;
    margin-left: 20rpx;
}

.icon {
    width: 28rpx;
    height: 28rpx;
    margin-left: 24rpx;
    margin-right: 16rpx;
}

.in-bar {
    color: #999;
}

.bar {
    height: 68rpx;
    width: 500rpx;
    font-size: 28rpx;
    display: inline-block;
    border-bottom-right-radius: 15px;
    border-top-right-radius: 15px;
}

.cancel-img {
    width: 28rpx;
    height: 28rpx;
    margin-right: 20rpx;
}

.cancel {
    width: 120rpx;
    line-height: 68rpx;
    text-align: center;
    display: inline-block;
    border: none;
}

